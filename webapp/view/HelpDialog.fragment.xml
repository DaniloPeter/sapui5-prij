<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog
        id="helpDialog"
        title="{i18n>openDialogButtonText}">
        <content>
            <VBox>

                <Button press=".onItemSelect" text="Пользователь 1" />
                <Button press=".onItemSelect" text="Пользователь 2" />
                <Button press=".onItemSelect" text="Пользователь 3" />

            </VBox>
        </content>
        <beginButton>
            <Button
                text="{i18n>dialogCloseButtonText}"
                press=".onCloseDialog" />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>